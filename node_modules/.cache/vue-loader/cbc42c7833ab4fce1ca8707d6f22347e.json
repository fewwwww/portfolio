 computed: {\n      generatedCommand () {\n        let genCommand = `${this.command.name}`\n        if (this.argv.length !== 0) {\n          genCommand = `${genCommand} ${this.argv.join(' ')}`\n        }\n        return genCommand\n      }\n    },\n    methods: {\n      /**\n       * _retrieve and execute the appropriate command_\n       */\n      execute () {\n        this.runCommand({\n          command: this.generatedCommand\n        })\n      },\n\n      handleClick () {\n        this.execute()\n      },\n\n      ...mapActions('portfolio', [\n        'runCommand'\n      ])\n    }\n  }\n</script>\n"]}]}