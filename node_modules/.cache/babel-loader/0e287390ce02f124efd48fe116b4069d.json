A,CAnDA;AAwDA,EAAA,OAxDA,qBAwDA;AACA,SAAA,IAAA,GAAA,KAAA,IAAA;AACA,GA1DA;AA2DA,EAAA,OA3DA,qBA2DA;AACA,QAAA,KAAA,OAAA,EAAA;AACA,WAAA,iBAAA,CAAA;AACA,QAAA,iBAAA,EAAA,CAAA,KAAA;AADA,OAAA,EADA,CAGA;;AAEA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA,MADA;AAEA,QAAA,MAAA,EAAA;AACA,UAAA,IAAA,EAAA,KAAA,IAAA,CAAA;AADA;AAFA,OAAA,WAKA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,sBAAA,EAAA,CACA;AACA;AACA,OATA;AAUA;AACA;AA5EA,CAAA","sourcesContent":["<template>\n  <span\n    v-if=\"isFound\"\n    class=\"vim\">\n    &nbsp;\n  </span>\n\n  <div\n    v-else\n    class=\"vim\">\n    <strong>{{ args.filename }}</strong> is not a valid file.\n  </div>\n</template>\n\n<script>\n  import { mapMutations, mapGetters } from 'vuex'\n\n  import argumented from '@/mixins/argumented'\n\n  /**\n   * This command opens the specified file 'filename' in a view of its own.\n   */\n  export default {\n    name: 'Vim',\n    mixins: [\n      argumented\n    ],\n    argSpec: {\n      args: [\n        {\n          name: 'filename',\n          type: String,\n          required: true\n        }\n      ],\n      kwargs: []\n    },\n    props: {\n      /**\n       * _the arguments passed to the command_\n       */\n      argv: {\n        type: Array\n      }\n    },\n    computed: {\n      /**\n       * _the file whose contents must be displayed_\n       */\n      file () {\n        return this.nodeLocatedAt(this.args.filename)\n      },\n      /**\n       * _whether a file matching the path was found_\n       */\n      isFound () {\n        return this.node && this.node.isFile()\n      },\n      /**\n       * _the imported HTML of the file_\n       */\n      path () {\n        const dirName = this.node.parent.name\n        const fileName = this.node.name\n        const filePath = `${dirName}/${fileName}.content.html`\n\n        return require(`@/assets/content/${filePath}`)\n      },\n\n      ...mapGetters('portfolio', [\n        'nodeLocatedAt'\n      ])\n    },\n    methods: {\n      ...mapMutations('portfolio', [\n        'clearSelectOutput'\n      ])\n    },\n    created () {\n      this.node = this.file\n    },\n    mounted () {\n      if (this.isFound) {\n        this.clearSelectOutput({\n          deletedComponents: ['Vim']\n        }) // Prevents redirect when going back\n\n        this.$router.push({\n          name: 'work',\n          params: {\n            slug: this.node.name\n          }\n        }).catch(err => {\n          if (err.name === 'NavigationDuplicated') {\n            // Do nothing\n          }\n        })\n      }\n    }\n  }\n</script>\n"],"sourceRoot":"src/templates/portfolio/commands"}]}